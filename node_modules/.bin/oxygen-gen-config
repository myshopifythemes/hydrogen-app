#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  --no-warnings "$basedir/../@shopify/mini-oxygen/dist/gen-config.js" "$@"
  ret=$?
else 
  node  --no-warnings "$basedir/../@shopify/mini-oxygen/dist/gen-config.js" "$@"
  ret=$?
fi
exit $ret
